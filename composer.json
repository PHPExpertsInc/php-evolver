{
    "name": "petercoles/gao",
    "description": "A Laravel package for performing Genetic Algorithm Optimisation",
    "keywords": [
        "GAO",
        "Genetic Algorithm Optimisation",
        "AI",
        "Artifical Intelligence"
    ],
    "homepage": "https://github.com/petercoles/laraGAO",
    "license": "MIT",
    "authors": [
        {
            "name": "Peter Coles",
            "email": "peterdcoles@gmail.com",
            "homepage": "https://petercoles.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "PeterColes\\GAO\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PeterColes\\GAO\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "PeterColes\\GAO\\GAOServiceProvider"
            ]
        }
    }
}
